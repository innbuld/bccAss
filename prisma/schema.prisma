generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Asset {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  symbol    String
  imageUrl  String
  price     Float
  hour      Float
  thour     Float
}
