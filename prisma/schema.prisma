
datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
  }
  
  generator client {
    provider = "prisma-client-js"
  }
  
  model Asset {
    id        String  @id @default(cuid())
    name      String
    symbol    String
    imageUrl  String
    price     Float
    hour      Float
    thour     Float
  }